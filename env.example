# Portfolio Website Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration (if needed)
VITE_API_URL=https://your-api.com

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_VERCEL_ANALYTICS=true

# Contact Form (if implemented)
VITE_CONTACT_EMAIL=contact@dietrich.tv
VITE_EMAILJS_SERVICE_ID=your_service_id
VITE_EMAILJS_TEMPLATE_ID=your_template_id
VITE_EMAILJS_PUBLIC_KEY=your_public_key

# Social Media Links
VITE_INSTAGRAM_URL=https://instagram.com/yourusername
VITE_BEHANCE_URL=https://behance.net/yourusername
VITE_LINKEDIN_URL=https://linkedin.com/company/yourusername

# Video/Media Configuration
VITE_VIMEO_ACCESS_TOKEN=your_vimeo_token
VITE_MEDIA_CDN_URL=https://your-cdn.com

# Development
VITE_NODE_ENV=development
